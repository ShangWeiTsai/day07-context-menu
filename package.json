{
	"name": "day07-context-menu",
	"displayName": "day07-context-menu",
	"description": "context-menu-practice",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "day07-context-menu.panelVisible",
				"title": "Showing when panel is visible"
			},
			{
				"command": "day07-context-menu.panelVisibleAlt",
				"title": "Showing alterative command when panel is invisible"
			},
			{
				"command": "day07-context-menu.panelInvisible",
				"title": "Showing when panel is invisible"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "day07-context-menu.panelVisible",
					"alt": "day07-context-menu.panelVisibleAlt",
					"when": "activePanel",
					"group": "navigation"
				},
				{
					"command": "day07-context-menu.panelInvisible",
					"when": "!activePanel",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
